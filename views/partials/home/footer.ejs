<script>
  window.onload = function() {
    setTimeout(function() {
      const err = document.getElementById('signup-error');
      err.remove();
      const inputs = document.querySelectorAll('input');
      inputs.forEach(input => {
        input.value = '';
      });
    }, 3000);
  };
  window.addEventListener("unload", (event) => {
    fetch('/logout', {
      method: 'POST',
      headers: {
        'Content-Type': 'application/json'
      },
      // body: JSON.stringify({
      //   logout: true
      // })
    });
  });
</script>
