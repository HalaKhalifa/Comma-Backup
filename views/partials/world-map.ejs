<div id="dashboard_world_map_chart"></div>
<script src="js/markers.utils.js"></script>
<script>
    document.addEventListener("DOMContentLoaded", function() {
        var map = new jsVectorMap({
            map: "world",
            selector: "#world_map",
            regionsSelectable: true,
            zoomButtons: true,
            markers: markers,
            markerStyle: {
                initial: {
                    r: 9,
                    strokeWidth: 7,
                    stokeOpacity: .4,
                    fill: window.theme.primary
                },
                hover: {
                    fill: window.theme.primary,
                    stroke: window.theme.primary
                }
            },
            zoomOnScroll: false,
            series: {
regions: [{
  attribute: "fill",
  legend: {
    title: "Visitors Location",
  },
  scale: {
    Palestine: "#08d191",
    Jordan: "#c79efd",
    "USA": "#ffc371",
    Egypt: "#08d191",
  },
  values: {
    // But when dealing with regions's series you should specify the region key.
    PS: "Palestine",
    US: "USA",
    IL: "Palestine",
    EG: "Egypt",
  },
}]
}
        });
        window.addEventListener("resize", () => {
            map.updateSize();
        });
    });
</script>